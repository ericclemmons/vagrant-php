{
    "name":         "ericclemmons/vagrant-php",
    "description":  "Instantly setup & launch PHP5.x Vagrant machine",
    "homepage":     "https://github.com/ericclemmons/vagrant-php",
    "keywords":     ["vagrant", "php", "generate", "generator"],
    "license":      "MIT",
    "authors": [
        {
            "name": "Eric Clemmons",
            "email": "eric@smarterspam.com"
        }
    ],
    "require": {
        "guzzle/guzzle": "*",
        "symfony/console": "2.1.*",
        "symfony/css-selector": "2.1.*",
        "symfony/dom-crawler": "2.1.*",
        "twig/twig": "1.12.*"
    },
    "autoload": {
        "psr-0": {
            "Vagrant": "src/",
            "Vagrant\\Tests": "tests/"
        }
    },
    "minimum-stability": "dev"
}
